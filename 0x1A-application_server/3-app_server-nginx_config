server {
    listen 80;
    server_name 18.234.129.180; # replace with your domain name

    location / {
        proxy_pass http://127.0.0.1:5000; # change to the port your Flask app is listening on
        proxy_set_header Host $host;
        proxy_set_header X-Real-IP $remote_addr;
    }

    location /airbnb-dynamic/number_odd_or_even/ {
        proxy_pass http://127.0.0.1:5001; # change to the port your Gunicorn app is listening on
        proxy_set_header Host $host;
        proxy_set_header X-Real-IP $remote_addr;
    }
}

